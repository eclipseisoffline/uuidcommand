{
  "schemaVersion": 1,
  "id": "uuidcommand",
  "version": "${version}",
  "name": "UUID Command",
  "description": "Adds a /uuid command to Minecraft",
  "authors": [
    {
      "name": "eclipseisoffline",
      "contact": {
        "homepage": "https://github.com/eclipseisoffline"
      }
    }
  ],
  "contact": {
    "homepage": "https://github.com/eclipseisoffline/uuidcommand",
    "issues": "https://github.com/eclipseisoffline/uuidcommand/issues",
    "sources": "https://github.com/eclipseisoffline/uuidcommand"
  },
  "license": "GPLv3",
  "icon": "assets/uuidcommand/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "xyz.eclipseisoffline.uuidcommand.UUIDCommand"
    ],
    "main": [
      "xyz.eclipseisoffline.uuidcommand.UUIDCommand"
    ]
